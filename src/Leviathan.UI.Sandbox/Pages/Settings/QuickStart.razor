@page "/Settings/QuickStart"


@if (Profiles != null) {
	<table class="table">
		<thead>
			<tr>
				<th>Name</th>
				<th>Type id</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			@foreach (var i in Profiles) {
				<tr>
					<td>@i.Name</td>
					<td>@i.Description</td>
					<td><button @onclick="(() => ApplyProfile(i.Id))">APPLY PROFILE</button></td>
				</tr>
			}
		</tbody>
	</table>
}